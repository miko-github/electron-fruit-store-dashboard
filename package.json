{
	"name": "electron-quick-start",
	"version": "1.0.0",
	"description": "A minimal Electron application",
	"main": "main.js",
	"scripts": {
		"start": "electron .",
		"sass:build": "./node_modules/sass/sass.js ./src/style/main.scss:./public/styles.css",
		"sass:watch": "./node_modules/sass/sass.js --watch ./src/style/main.scss:./public/styles.css",
		"watch": "npm run sass:watch & npm run start",
		"dev": "npm run watch"
	},
	"repository": "https://github.com/electron/electron-quick-start",
	"keywords": [
		"Electron",
		"quick",
		"start",
		"tutorial",
		"demo"
	],
	"author": "GitHub",
	"license": "MIT",
	"devDependencies": {
		"@yek/sass": "^3.2.0",
		"electron": "^19.0.1",
		"sass": "^1.52.2"
	},
	"dependencies": {
		"normalize.css": "^8.0.1",
		"sqlite3": "^5.0.8"
	}
}
